{
    "name": "yafimm/rajaongkir",
    "description": "Klien API PHP untuk RajaOngkir",
    "keywords": [
        "rajaongkir",
        "laravel"
    ],
    "homepage": "https://github.com/yafimm/rajaongkir",
    "license": "MIT",
    "authors": [
        {
            "name": "Ian Mustafa",
            "email": "mail@ianmustafa.com",
            "role": "Maintainer"
        },
        {
            "name": "Yafi",
            "email": "me@yafimm.com",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.26.2",
        "spatie/phpunit-watcher": "^1.12",
        "fzaninotto/faker": "^1.8",
        "orchestra/testbench": "^4.1",
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Yafimm\\RajaOngkir\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yafimm\\RajaOngkir\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "watch": "vendor/bin/phpunit-watcher"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yafimm\\RajaOngkir\\Providers\\LaravelServiceProvider"
            ],
            "aliases": {
                "RajaOngkir": "Yafimm\\RajaOngkir\\Facades\\RajaOngkir"
            }
        }
    }
}
